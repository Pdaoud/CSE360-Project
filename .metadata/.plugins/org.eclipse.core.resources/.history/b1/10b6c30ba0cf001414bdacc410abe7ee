import javax.swing.JPanel;
import javax.swing.JTextArea;

public class CheckCompletedPanel extends JPanel {

	private JPanel textArea;
	private JTextArea myArea;
	/**
	 * Create the panel.
	 */
	public CheckCompletedPanel() //Eventually will have an array or Vector parameter with form objects
	{
		textArea = new JPanel();//initialize
		myArea = new JTextArea("Will show the list of completed forms via an Array or Vector of Form Objects.", 20, 20);
		//initialize
		myArea.setEditable(false); //set the text area to un-editable
		textArea.add(myArea); //add component
		this.add(textArea); // add to constructor
	}
}
